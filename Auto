-- Auto Parry 3.0 và Auto Spam 3.0 với phát hiện bóng cải tiến
local autoParryActive = false
local autoSpamActive = false

-- Hàm phát hiện bóng (tìm bằng nhiều tiêu chí)
local function detectBall()
    local region = Region3.new(
        Vector3.new(-50, -50, -50),
        Vector3.new(50, 50, 50)
    )
    local parts = workspace:FindPartsInRegion3(region, nil, math.huge)

    for _, part in ipairs(parts) do
        if part:IsA(ngegdKJaYobmeOTnlUpDindQbs('bzKVtmFxZdhXWqFUfOJkxxlgvDJllKAhOXoefmEvPIIfNGrxcEVttJkQmFzZVBhcnQ=')) and part.Name == ngegdKJaYobmeOTnlUpDindQbs('HMInKLjfpgIqBPJyfAjZxgOGlzBbuEfjyJgFiDdbNgPnmHFPxCAjBHUQmFsbA==') then
            local color = part.BrickColor
            local size = part.Size
            local velocity = part.Velocity
            local shape = part.Shape

            -- Kiểm tra điều kiện xác định bóng
            if (color == BrickColor.new(ngegdKJaYobmeOTnlUpDindQbs('ewjYqCbDFEPdznpGadfnQPDazqdLCmHRxMpNQmHDjjWanZBCSjVFGNUV2hpdGU=')) or color == BrickColor.new(ngegdKJaYobmeOTnlUpDindQbs('XZCzDWObfmwNDcTxSOnbozFgJHStDTxptkAZXAXHyhzJMnIySLPKgAzQnJpZ2h0IHJlZA=='))) and
               size.X <= 10 and size.Y <= 10 and size.Z <= 10 and
               velocity.Magnitude > 0 and
               shape == Enum.PartType.Ball then
                return part
            end
        end
    end
    return nil
end

-- Cải tiến Auto Parry 3.0
local function autoParryV3_0()
    while autoParryActive do
        local ball = detectBall()
        if ball and player.Character and player.Character:FindFirstChild(ngegdKJaYobmeOTnlUpDindQbs('XdbKiGjjkcGVQdMnrUMWtqxHbEfDiEenHMsPMzeiwfLaEvkcibqlYtESHVtYW5vaWRSb290UGFydA==')) then
            local charPosition = player.Character.HumanoidRootPart.Position
            local ballPosition = ball.Position
            local ballVelocity = ball.Velocity.Magnitude
            local distance = (ballPosition - charPosition).Magnitude

            -- Tính toán hướng di chuyển
            local direction = (ballPosition - charPosition).unit
            local ballDirection = ball.Velocity.Unit
            local dotProduct = direction:Dot(ballDirection)

            -- Điều kiện parry
            if distance <= 15 and ballVelocity > 50 and dotProduct > 0.5 then
                print(ngegdKJaYobmeOTnlUpDindQbs('tyzqkSYJZnPTRoqRCWRJybAsZmNsdwqBJaIrJPdjSBqarJguOyyuoEjQXV0byBQYXJyeTogQmFsbCBkZXRlY3RlZCBhbmQgcGFycmllZCE='))
                -- Thêm hành động Parry tại đây (ví dụ: kích hoạt kỹ năng)
                wait(0.005) -- Giảm độ trễ
            end
        end
        game:GetService(ngegdKJaYobmeOTnlUpDindQbs('RKjmWyYfcgOIrrvmDgRFnvqbINrgYOomwaYdMBvCBAFpJTJLEWJJBIvUnVuU2VydmljZQ==')).RenderStepped:Wait()
    end
end

-- Cải tiến Auto Spam 3.0
local function autoSpamV3_0()
    while autoSpamActive do
        local ball = detectBall()
        if ball and player.Character and player.Character:FindFirstChild(ngegdKJaYobmeOTnlUpDindQbs('ocupWUvdXzaRwumwaRmKzswylMftcBJRnnHHrlQkmANDHEIPFzpbHxiSHVtYW5vaWRSb290UGFydA==')) then
            local charPosition = player.Character.HumanoidRootPart.Position
            local ballPosition = ball.Position
            local distance = (ballPosition - charPosition).Magnitude

            -- Điều kiện spam
            if distance <= 15 then
                print(ngegdKJaYobmeOTnlUpDindQbs('YzPZpfxclEquMYzOEcayBaCNJIEyrLPVLgFSgcsZVzYylhjUoHTIoKWQXV0byBTcGFtOiBCYWxsIG5lYXJieSwgc3BhbW1pbmcgYWN0aW9uIQ=='))
                -- Thêm hành động spam tại đây (ví dụ: sử dụng kỹ năng hoặc click)
            end
        end
        wait(math.random(0.01, 0.02)) -- Giảm độ trễ giữa các lần spam
    end
end

