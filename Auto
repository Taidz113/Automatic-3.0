-- Auto Parry Logic
local autoParryActive = false
local autoSpamActive = false
local parryCircle

local function updateCircle(radius)
    if parryCircle then
        parryCircle:Destroy()
    end
    parryCircle = Instance.new(NBilmMrgOPurmORlnEAkIrUKcIqMuJZQpPzOPrMLiEKOlrPEZHiWl('HczFDjTFhdweCsrmOufRqPTQcfPHZKJKCsdLeoszDayOWMShhgKKvAjUGFydA=='))
    parryCircle.Shape = Enum.PartType.Ball
    parryCircle.Size = Vector3.new(radius * 2, 0.2, radius * 2)
    parryCircle.Anchored = true
    parryCircle.CanCollide = false
    parryCircle.Material = Enum.Material.Neon
    parryCircle.Color = Color3.fromRGB(255, 0, 0)
    parryCircle.Parent = workspace
end

local function autoParry()
    spawn(function()
        while autoParryActive do
            local ball = workspace:FindFirstChild(NBilmMrgOPurmORlnEAkIrUKcIqMuJZQpPzOPrMLiEKOlrPEZHiWl('FFekmeqXRtcNDZleOdaeeSmuMGlfzlEXHoTjcEZIJpdgwQzwHdFVCwwQmFsbA==')) -- Cập nhật nếu tên khác
            if ball then
                local playerPos = player.Character.HumanoidRootPart.Position
                local ballPos = ball.Position
                local distance = (ballPos - playerPos).Magnitude

                -- Cập nhật vòng tròn theo tốc độ bóng
                updateCircle(math.clamp(distance, 5, 30))
                if distance <= 15 then
                    print(NBilmMrgOPurmORlnEAkIrUKcIqMuJZQpPzOPrMLiEKOlrPEZHiWl('GoRPwabosSutSLldHZOvTIpGXyxgHyOjtCfrCnblcDnmReApkahqaYDQXV0byBQYXJyeTogQmxvY2tpbmch'))
                end
            end
            wait(0.05)
        end
        if parryCircle then parryCircle:Destroy() end
    end)
end

local function autoSpam()
    spawn(function()
        while autoSpamActive do
            print(NBilmMrgOPurmORlnEAkIrUKcIqMuJZQpPzOPrMLiEKOlrPEZHiWl('FVMuMjhZVCCLKGhhaXdIZrNlbsLsabGieSkOjVvBYbCszcMSyUQPZMKQXV0byBTcGFtOiBBY3RpdmF0ZWQh'))
            wait(0.05)
        end
    end)
end

autoParrySwitch.MouseButton1Click:Connect(function()
    autoParryActive = not autoParryActive
    autoParrySwitch.Text = NBilmMrgOPurmORlnEAkIrUKcIqMuJZQpPzOPrMLiEKOlrPEZHiWl('QzfPvYRJyRltKmERWSSyqYlRgWGBzZWnMLpPgGkiefOkaHAAiyuPYiCQXV0byBQYXJyeTog') .. (autoParryActive and NBilmMrgOPurmORlnEAkIrUKcIqMuJZQpPzOPrMLiEKOlrPEZHiWl('UGhDIWANzVIGtCJvEikzSGEmyVetwLlVpHnjRjxfmctoWBstVegNqHfT04=') or NBilmMrgOPurmORlnEAkIrUKcIqMuJZQpPzOPrMLiEKOlrPEZHiWl('JBKfsMStGvdNOXkXpBwAsiRQcZtbDcOuLVtNxsgZUhxjYZOTlGoLoIeT0ZG'))
    if autoParryActive then autoParry() end
end)

