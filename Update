-- Cải tiến Auto Parry 3.1 (Coordinate-based Detection)
local autoParryActive = false
local function autoParryV3_1()
    while autoParryActive do
        local ball = game.Workspace:FindFirstChild(StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('lugiCnNRIOTCjfGwepAqjAWUICVchgUezizyOrSWHmweoVwtYaMEeRnQmFsbA=='))
        if ball and player.Character and player.Character:FindFirstChild(StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('BOaXGUzuDMSuwkzKADqSotXaoNwUyjqlfdUxgSKzAdvvqwQVktKPbTdSHVtYW5vaWRSb290UGFydA==')) then
            local charPosition = player.Character.HumanoidRootPart.Position
            local ballPosition = ball.Position
            local ballVelocity = ball:FindFirstChild(StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('kdPyMsByhvcuXYUaAklnzrXJdcWmHcfcKobfqqVVPXjTqqVYZCMfqSkVmVsb2NpdHk=')) and ball.Velocity.Magnitude or 0
            local distance = (ballPosition - charPosition).Magnitude

            -- Xác định nếu bóng đang tiến về phía người chơi
            local direction = (ballPosition - charPosition).unit
            local ballDirection = ball.Velocity.Unit
            local dotProduct = direction:Dot(ballDirection)

            -- Nếu bóng gần và đang di chuyển về phía người chơi
            if distance <= 15 and ballVelocity > 50 and dotProduct > 0.5 then
                print(StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('JlcPqAtonXXGLAiAgSzgMFbyjYWXyixQIFCtBiOkuJOUqzApwzfpXoaQXV0byBQYXJyeTogQmFsbCBpbmNvbWluZyE='))
                -- Thực hiện hành động Parry tại đây (ví dụ: gọi hàm Parry)
                wait(0.005) -- Giảm độ trễ
            end
        end
        game:GetService(StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('cFJqLzORRiVqRrtIPAVAKVXIDJBykDzvQzigqFuawdZGSHJGEFcQBBqUnVuU2VydmljZQ==')).RenderStepped:Wait()
    end
end
autoParrySwitch.MouseButton1Click:Connect(function()
    autoParryActive = not autoParryActive
    autoParrySwitch.Text = StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('yLGvHSxRAKqQMkddVZKLCWgnAAEjveqJiMvkfJIRnzaytwZyVfDgHshQXV0byBQYXJyeTog') .. (autoParryActive and StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('XKmYhlkRpwfulFRxziqsLeQnpdvBArjgplbzPFBGMSRPxQZlTQrMBVLT04=') or StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('NKfAsXOQZPfcIxrQvLCAFottfkbIjzqOabANdztQajSVHCNFryISqGOT0ZG'))
    if autoParryActive then spawn(autoParryV3_1) end
end)

-- Auto Spam 3.1 (Coordinate-based Detection)
local autoSpamActive = false
local function autoSpamV3_1()
    while autoSpamActive do
        print(StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('PVnFPXWQozNNVDLwGLBILJzgaJYfQyXZaETrcvGuYRbKJZiygfbwdwLQXV0byBTcGFtIEFjdGl2ZSE='))
        local ball = game.Workspace:FindFirstChild(StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('PExWLkvlECAGRIdKgXBDTbuXkSCpgioAluFVBNwXxKXOSikFwfBoXaCQmFsbA=='))
        if ball and player.Character then
            local ballPosition = ball.Position
            local charPosition = player.Character.HumanoidRootPart.Position
            local distance = (ballPosition - charPosition).Magnitude

            -- Spam nếu bóng gần và có thể dễ dàng spam khi bóng còn một khoảng cách ngắn
            if distance <= 15 then
                print(StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('KNRaDlCZgKkdakslRPlTbxQBocBXWQcoSyKFFCJWpEwAAIJZXqDCKgOU3BhbW1pbmcgYXQgQmFsbCE='))
                -- Thực hiện hành động spam tại đây (ví dụ: kích hoạt kỹ năng)
            end
        end
        wait(math.random(0.01, 0.02)) -- Tùy chỉnh độ trễ để spam tự nhiên
    end
end
autoSpamSwitch.MouseButton1Click:Connect(function()
    autoSpamActive = not autoSpamActive
    autoSpamSwitch.Text = StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('mckMKSpbxYuPGNftawIrFRXIMxqJAfwiijNRWaRiqgVVKemdJzGfyPMQXV0byBTcGFtOiA=') .. (autoSpamActive and StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('HMnPBInPdYihIoHfhRFUKtyRHGUPHwvPHwVcLSggKMqODKELMxnaQAXT04=') or StiemitbXAyQmodhYYsTkvsWwiTpdZNGjAlmjclmXVieyYOzjwVxmJNNbitiIUNewKEoIWbbWBwvxOakYCJQqdKtAszpBDXq('imCiPOaibIfzcYzJvJGcMJRmXxkfuGfivpEDwrqXdfWlKLphuoOEXtCT0ZG'))
    if autoSpamActive then spawn(autoSpamV3_1) end
end)

