-- Vòng tròn Auto Parry
local function createCircle(radius)
    if circle then circle:Destroy() end
    circle = Instance.new(qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('lBDyFjyPVFzdjQpDNFwiTFEfhcGwIYPJSIkkxlvvvMsBJSYoEDJMrDiUGFydA=='))
    circle.Shape = Enum.PartType.Ball
    circle.Size = Vector3.new(radius * 2, 0.2, radius * 2)
    circle.Anchored = true
    circle.CanCollide = false
    circle.Material = Enum.Material.Neon
    circle.Color = Color3.fromRGB(255, 0, 0)
    circle.Parent = workspace
end

local function updateCircle()
    if circle and player.Character and player.Character:FindFirstChild(qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('pBCEsOuhMejwxIPJynExSXWcMEAuOoiBmfuKdPUxXUwcBQoRMKoXrwaSHVtYW5vaWRSb290UGFydA==')) then
        circle.Position = player.Character.HumanoidRootPart.Position
    end
end

-- Auto Fix Lag Logic
local autoFixLagActive = false
autoFixLagSwitch.MouseButton1Click:Connect(function()
    autoFixLagActive = not autoFixLagActive
    autoFixLagSwitch.Text = qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('RzjnfmRsxrUnzZPgOlpeeuqYfFnzWrVPRKMIeqefNYjKtxQITbmPWTwQXV0byBGaXggTGFnOiA=') .. (autoFixLagActive and qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('ClvgrgVFaPIlgyKPLsWGbXBqckuXromAekXQEZpFzNJRYGemWJBQRGAT04=') or qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('ZTgGlokSoTdQyPaODSwyajzxhdJgikulUwnvKYXKStrRRqOyTEYPFwpT0ZG'))
    if autoFixLagActive then
        settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
        game.Lighting.Brightness = 0
        game.Lighting.GlobalShadows = false
    else
        settings().Rendering.QualityLevel = Enum.QualityLevel.Automatic
        game.Lighting.Brightness = 2
        game.Lighting.GlobalShadows = true
    end
end)

-- Auto Parry 4.0 Pro
local autoParryActive = false
local function autoParry()
    createCircle(15) -- Kích thước mặc định
    spawn(function()
        while autoParryActive do
            if player.Character and player.Character:FindFirstChild(qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('znaHxousVGhlIpXNBiBNUQSxOspdPxtPGoClOneNmULzeDVCEfkbtVYSHVtYW5vaWRSb290UGFydA==')) then
                local ball = workspace:FindFirstChild(qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('CVYXBiVUbADTCtpXlFUAXyZBVjVpOHnmWYXjHcDqstUdsPFQIfDQObgQmFsbA==')) -- Cập nhật nếu tên khác
                if ball then
                    local playerPos = player.Character.HumanoidRootPart.Position
                    local ballPos = ball.Position
                    local distance = (ballPos - playerPos).Magnitude
                    createCircle(math.clamp(distance, 5, 30)) -- Thay đổi kích thước vòng tròn

                    if distance <= 15 then
                        print(qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('bipjTBryKRgZiIobHboKvZWIslERmJHgoZBHXFcXCCVEtoiTupPYTsAQXV0byBQYXJyeTogQmxvY2tpbmch'))
                        -- Thực hiện hành động Block (nếu có)
                    end
                end
                updateCircle()
            end
            wait(0.05)
        end
        if circle then circle:Destroy() end
    end)
end

autoParrySwitch.MouseButton1Click:Connect(function()
    autoParryActive = not autoParryActive
    autoParrySwitch.Text = qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('QBFTPWMEIStvqraWNkiuInlefJdBixptsBTyQGPrWRBTjgGdADgGpAcQXV0byBQYXJyeTog') .. (autoParryActive and qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('CFkGeOtsZPjgBomzdJIDXIzWpLwpVpkiOMzVnxTotvPihTXTckwwTSDT04=') or qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('cbOwLGAWfHWGfwASoDBNGEZKAqpnVrRSoGhifYSxydQJZIqeBvdCeTfT0ZG'))
    if autoParryActive then autoParry() end
end)

-- Auto Spam 4.0 Pro
local autoSpamActive = false
local function autoSpam()
    spawn(function()
        while autoSpamActive do
            print(qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('YMHFgJzqdxRCKPuRWQdoDrSDBTzqSOGvSxxrySLXOPnOHcdPZrFWfrHQXV0byBTcGFtOiBTcGFtbWluZyBhY3Rpb24h'))
            wait(0.05)
        end
    end)
end

