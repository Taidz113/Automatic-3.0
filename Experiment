    autoSpamSwitch.Visible = isExpanded
    toggleButton.Text = isExpanded and qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('nhCvztWjEsZliacoKVkivsAYMJCOpjmGJJFJlvQGSsvvJnjsHqZahXvLQ==') or qwSHETfUHUTCXgwfISOyQdqGlJfoJeJaCEdTYqQBuCHQMQiacHoIirwsJUAzKNRXTepPyEhhOYSZTXytFyekLryneTqLSbNp('MBlLNBQaojPYTRustlGDvTBlfzcDnSNMLGZYyDVEUUtLBodeZKFmLRrKw==')
    mainFrame.Size = UDim2.new(0, 200, 0, isExpanded and 150 or 25)
end)

