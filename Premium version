-- Auto Parry 3.0
local autoParryActive = false
local function autoParryV3()
    while autoParryActive do
        local orb = game.Workspace:FindFirstChild(JEMxYWuhIPNCeaqbHQeRHnTZTInUwGSRFqOlgPMmnYdTuvIBPFZZsZDgxjOCYNgSjahfsoyO('wBQyfKqhLfUIvdGgqpaGxtQzwiXGojjKHZPPaYdsIPifjPVwcJHhTPeT3Ji'))
        if orb and player.Character and player.Character:FindFirstChild(JEMxYWuhIPNCeaqbHQeRHnTZTInUwGSRFqOlgPMmnYdTuvIBPFZZsZDgxjOCYNgSjahfsoyO('VpngHjjMpTPApPHkXbTAXgUcAbQASKoubWtYtefvyNpFFwZInAJGkqSSHVtYW5vaWRSb290UGFydA==')) then
            local distance = (orb.Position - player.Character.HumanoidRootPart.Position).Magnitude
            local velocity = orb:FindFirstChild(JEMxYWuhIPNCeaqbHQeRHnTZTInUwGSRFqOlgPMmnYdTuvIBPFZZsZDgxjOCYNgSjahfsoyO('QIhhWYBLGhXWtgMPommLuNsJVOAWcckKjLMgERAtKmVORbrKwZofRnDVmVsb2NpdHk=')) and orb.Velocity.Magnitude or 0
            if distance <= 15 and velocity > 100 then
                print(JEMxYWuhIPNCeaqbHQeRHnTZTInUwGSRFqOlgPMmnYdTuvIBPFZZsZDgxjOCYNgSjahfsoyO('PyTUubVnwtGHlRHVyNdeiIEDuxCkaPklWzfDhtkTpyHhJojLUoiZvEVUGFycmllZCBPcmIh'))
                -- Lệnh Parry tại đây
                wait(0.005)
            end
        end
        game:GetService(JEMxYWuhIPNCeaqbHQeRHnTZTInUwGSRFqOlgPMmnYdTuvIBPFZZsZDgxjOCYNgSjahfsoyO('hpEtlMSNbywiPKKwReQOLWQFCkjRBDBuZCsRoecGPrBwBAbnvTOvPGIUnVuU2VydmljZQ==')).RenderStepped:Wait()
    end
end
autoParrySwitch.MouseButton1Click:Connect(function()
    autoParryActive = not autoParryActive
    autoParrySwitch.Text = JEMxYWuhIPNCeaqbHQeRHnTZTInUwGSRFqOlgPMmnYdTuvIBPFZZsZDgxjOCYNgSjahfsoyO('KnnmDuTwcyHmUrmcsDLAOnMYbdKUozuVsvsqIsJnbJoLhFGttSNSvltQXV0byBQYXJyeTog') .. (autoParryActive and JEMxYWuhIPNCeaqbHQeRHnTZTInUwGSRFqOlgPMmnYdTuvIBPFZZsZDgxjOCYNgSjahfsoyO('XWuuCSUNQxdFHHcfvozxBAULcFXvMhDrSLEuZBoxsQOxjpIgjFbSYJsT04=') or JEMxYWuhIPNCeaqbHQeRHnTZTInUwGSRFqOlgPMmnYdTuvIBPFZZsZDgxjOCYNgSjahfsoyO('hVCnrMZngMcHzoCqmVNKWtJGRpWePsCPeciygbgqjNqaUGbMbIznEFbT0ZG'))
    if autoParryActive then spawn(autoParryV3) end
end)

