local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
function fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn(data) m=string.sub(data, 0, 55) data=data:gsub(m,'')

data = string.gsub(data, '[^'..b..'=]', '') return (data:gsub('.', function(x) if (x == '=') then return '' end local r,f='',(b:find(x)-1) for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end return r; end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x) if (#x ~= 8) then return '' end local c=0 for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end return string.char(c) end)) end


 


-- GUI di chuyển được
local gui = script.Parent -- Đảm bảo gắn đúng GUI
local userInput = game:GetService(fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn('zpnvgexpflIeqXgSMIBiZONinfzdLTxTzxniIPvpSKexaiIjxnCHixEVXNlcklucHV0U2VydmljZQ=='))
local dragging = false
local dragStart, startPos

local function update(input)
    local delta = input.Position - dragStart
    gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

gui.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = gui.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

gui.InputChanged:Connect(function(input)
    if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        update(input)
    end
end)

-- Auto Parry 3.0
local autoParryActive = false
local RunService = game:GetService(fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn('MTGOhIuGzykqzHwFyxQTcmaWkfhiEjyIgvtFRmJWBmWderKrJOubpZqUnVuU2VydmljZQ=='))

local function autoParryV3()
    spawn(function()
        while autoParryActive do
            local ball = game.Workspace:FindFirstChild(fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn('KrXhsQHiBlUMNbzJhscYSwMSrNshMlpDwToluqUcPXnQVoDBSzCeyEqQmFsbA==')) -- Tên đối tượng quả bóng
            if ball and player.Character and player.Character:FindFirstChild(fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn('RJndRcGpWgzhDVkAjVxCmRgkhnthOLeGogDMdJEaQkiTPYOyOtEjBdHSHVtYW5vaWRSb290UGFydA==')) then
                local ballPosition = ball.Position
                local charPosition = player.Character.HumanoidRootPart.Position
                local distance = (ballPosition - charPosition).Magnitude
                local velocity = ball:FindFirstChild(fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn('lTEsvnZLYwtEvCUrebQsxDIuyOJhmAAHzQiRnXBFcDFnyWQYXGdBIDLVmVsb2NpdHk=')) and ball.Velocity.Magnitude or 0

                -- Phản xạ nhanh khi bóng đến gần
                if distance <= 20 and velocity > 0 then
                    local timeToReact = distance / velocity -- Tính toán thời gian phản ứng
                    if timeToReact <= 0.1 then
                        print(fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn('wLiImNjcBkRHsuMjRYGfgEjhFDPGUmZPBOmzqMYUVUApkWtJWJnWYKIQXV0byBQYXJyeSAzLjA6IEJhbGwgaGl0IQ=='))
                        -- Thêm lệnh Parry vào đây
                    end
                    wait(0.01) -- Giảm độ trễ tối đa
                end
            end
            RunService.RenderStepped:Wait()
        end
    end)
end

parryButton.MouseButton1Click:Connect(function()
    autoParryActive = not autoParryActive
    if autoParryActive then
        parryButton.Text = fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn('DuxhHMvDysBqiyCQdFkLdmufaOTlrBTaieIFBsLlArcGwnyQyoIkjrVQXV0byBQYXJyeTogT04=')
        autoParryV3()
    else
        parryButton.Text = fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn('GqxfEHeWVcTgSnsBuHAiQlXlAbnTKpjKEXQCaEfljUsKobJKhYzkTJmQXV0byBQYXJyeTogT0ZG')
    end
end)

-- Auto Spam 3.0
local autoSpamActive = false

local function autoSpamV3()
    spawn(function()
        while autoSpamActive do
            print(fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn('cjdKGstndsPoVVwBdczzZWXQWHrnzxTicKjmOTDPUUIIKPQSEGzdLKbQXV0byBTcGFtIDMuMDogQ2xpY2sh'))
            -- Thực hiện hành động click (ví dụ: fire key hoặc kỹ năng)
            wait(math.random(0.02, 0.1)) -- Tùy chỉnh thời gian chờ linh hoạt
        end
    end)
end

spamButton.MouseButton1Click:Connect(function()
    autoSpamActive = not autoSpamActive
    if autoSpamActive then
        spamButton.Text = fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn('yKHRPkIOkLpJNuHkZmdobzLYzQkroHsEXzTWXEZzcstmyIjJlIpRwkgQXV0byBTcGFtOiBPTg==')
        autoSpamV3()
    else
        spamButton.Text = fsQpJbnUKcnNnWOwDTSresCEoMFJzpGdBEMytscnMBkjdNpNdjJnjhXFbUzMORgAAlZidnKGxn('peuvYNRZqzsZhkIoVhkhzmzLgWvrLlVfyjWEreYNholHfcKjAyGiQKRQXV0byBTcGFtOiBPRkY=')
    end
end)

-- Hiện lại menu sau khi nhân vật chết hoặc reset
player.CharacterAdded:Connect(function()
    gui.Enabled = true -- Đảm bảo GUI luôn hiển thị
end)
    
